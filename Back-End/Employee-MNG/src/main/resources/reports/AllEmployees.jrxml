<jasperReport name="EmployeeIndividual"
    language="java"
    columnCount="1"
    pageWidth="1650"
    pageHeight="842"
    columnWidth="1610"
    leftMargin="20"
    rightMargin="20"
    topMargin="20"
    bottomMargin="20"
	ignorePagination="true"
    whenNoDataType="AllSectionsNoDetail"
    uuid="d5495170-9fc8-427d-b523-24beb682fec1">

    <!-- PARAMETERS (converted from fields) -->
    <parameter name="employeeId" class="java.lang.String"/>
    <parameter name="salutation" class="java.lang.String"/>
    <parameter name="firstName" class="java.lang.String"/>
    <parameter name="middleName" class="java.lang.String"/>
    <parameter name="lastName" class="java.lang.String"/>
    <parameter name="gender" class="java.lang.String"/>
    <parameter name="dob" class="java.lang.String"/>
    <parameter name="mobile" class="java.lang.String"/>
    <parameter name="email" class="java.lang.String"/>
    <parameter name="designation" class="java.lang.String"/>
    <parameter name="employeeGroup" class="java.lang.String"/>
    <parameter name="reportingManager" class="java.lang.String"/>
    <parameter name="department" class="java.lang.String"/>
    <parameter name="status" class="java.lang.String"/>
    <parameter name="relievingDate" class="java.lang.String"/>
    <parameter name="site" class="java.lang.String"/>
    <parameter name="country" class="java.lang.String"/>
    <parameter name="state" class="java.lang.String"/>
    <parameter name="city" class="java.lang.String"/>
    <parameter name="zipCode" class="java.lang.String"/>
    <parameter name="addressLine1" class="java.lang.String"/>
    <parameter name="addressLine2" class="java.lang.String"/>

    <background splitType="Stretch"/>

    <!-- TITLE (UNCHANGED) -->
    <title height="97" splitType="Stretch">
        <element kind="staticText" x="220" y="2" width="390" height="30" fontSize="22.0">
            <text><![CDATA[New Employees Registration Report]]></text>
        </element>
    </title>

    <!-- DETAIL BAND -->
    <detail>
        <band height="55" splitType="Stretch">

            <element kind="textField" x="0" y="0" width="72" height="30">
                <expression><![CDATA[$P{employeeId}]]></expression>
            </element>

            <element kind="textField" x="72" y="0" width="138" height="30">
                <expression><![CDATA[
                    ($P{salutation} != null ? $P{salutation} : "") + " " +
                    ($P{firstName} != null ? $P{firstName} : "") + " " +
                    ($P{middleName} != null ? $P{middleName} : "") + " " +
                    ($P{lastName} != null ? $P{lastName} : "")
                ]]></expression>
            </element>

            <element kind="textField" x="210" y="0" width="69" height="30">
                <expression><![CDATA[$P{gender}]]></expression>
            </element>

            <element kind="textField" x="279" y="0" width="69" height="30">
                <expression><![CDATA[$P{dob}]]></expression>
            </element>

            <element kind="textField" x="348" y="0" width="69" height="30">
                <expression><![CDATA[$P{mobile}]]></expression>
            </element>

            <element kind="textField" x="417" y="0" width="100" height="30">
                <expression><![CDATA[$P{email}]]></expression>
            </element>

            <element kind="textField" x="517" y="0" width="100" height="30">
                <expression><![CDATA[$P{designation}]]></expression>
            </element>

            <element kind="textField" x="617" y="0" width="100" height="30">
                <expression><![CDATA[$P{employeeGroup}]]></expression>
            </element>

            <element kind="textField" x="717" y="0" width="100" height="30">
                <expression><![CDATA[$P{reportingManager}]]></expression>
            </element>

            <element kind="textField" x="817" y="0" width="100" height="30">
                <expression><![CDATA[$P{department}]]></expression>
            </element>

            <element kind="textField" x="917" y="0" width="100" height="30">
                <expression><![CDATA[$P{status}]]></expression>
            </element>

            <element kind="textField" x="1017" y="0" width="100" height="30">
                <expression><![CDATA[$P{relievingDate}]]></expression>
            </element>

            <element kind="textField" x="1117" y="0" width="100" height="30">
                <expression><![CDATA[$P{site}]]></expression>
            </element>

            <element kind="textField" x="1217" y="0" width="83" height="30">
                <expression><![CDATA[$P{city}]]></expression>
            </element>

            <element kind="textField" x="1300" y="0" width="80" height="30">
                <expression><![CDATA[$P{state}]]></expression>
            </element>

            <element kind="textField" x="1380" y="0" width="60" height="30">
                <expression><![CDATA[$P{country}]]></expression>
            </element>

            <element kind="textField" x="1440" y="0" width="70" height="30">
                <expression><![CDATA[$P{zipCode}]]></expression>
            </element>

            <element kind="textField" x="1510" y="0" width="100" height="30">
                <expression><![CDATA[
                    ($P{addressLine1} != null ? $P{addressLine1} : "") + " " +
                    ($P{addressLine2} != null ? $P{addressLine2} : "")
                ]]></expression>
            </element>

        </band>
    </detail>

</jasperReport>